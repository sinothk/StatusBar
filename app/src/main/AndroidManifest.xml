<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sinothk.helper.statusBar.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UseInFragmentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ColorStatusBarActivity"
            android:label="@string/app_name" />
        <activity android:name=".ImageStatusBarActivity" />
        <activity android:name=".ImageViewActivity" />
        <activity android:name=".test.DemoMainActivity" />
        <activity android:name=".SwipeBackActivity" />
        <activity android:name=".SwitchModeActivity" />

        <activity android:name=".MainActivity" />
    </application>

</manifest>